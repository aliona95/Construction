<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.construction">

    <application
        android:allowBackup="true"
        android:icon="@drawable/shapes_icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MenuActivity" />
        <activity android:name=".TriangleAreaActivity" />
        <activity android:name=".SquareAreaActivity" />
        <activity android:name=".CircleAreaActivity" />
        <activity android:name=".RectangleAreaActivity" />
        <activity android:name=".BallVolumeActivity" />
        <activity android:name=".CubeVolumeActivity" />
        <activity android:name=".CylinderVolumeActivity" />
        <activity android:name=".ConeVolumeActivity"></activity>
    </application>

</manifest>